<project>
  <pomVersion>3</pomVersion>
  <name>jspider-main</name>
  <id>jspider</id>
  <currentVersion>0-3-1-dev</currentVersion>
  <organization>
    <name>Gunther Van Roey</name>
    <url>http://www.javacoding.net</url>
    <logo>/images/jakarta-logo-blue.gif</logo>
  </organization>
  <inceptionYear>2002</inceptionYear>
  <package>net.javacoding.jspider</package>
  <logo>/images/blue-logo.gif</logo>

  <gumpRepositoryId>jakarta</gumpRepositoryId>
  <shortDescription>Java Web Spider</shortDescription>
  <description>JSpider is a Java implementation of a Web spider engine</description>
  <url>http://j-spider.sourceforge.net</url>
  <issueTrackingUrl>http://sourceforge.net/tracker/?atid=511632&amp;group_id=65617&amp;func=browse</issueTrackingUrl>
  <siteAddress>j-spider.sourceforge.net</siteAddress>
  <siteDirectory>/www/spider/</siteDirectory>

 <distributionDirectory>/www/spider/builds/jspider-main/</distributionDirectory>
  <repository>
    <connection>scm:cvs:pserver:anonymous@cvs.j-spider.sourceforge.net:/cvsroot/j-spider</connection>
    <url>http://cvs.sourceforge.net/cgi-bin/viewcvs.cgi/j-spider/jspider-main/</url>
  </repository>
  <releases>
    <release>
      <version>0-1-0-dev</version>
      <tag>jspider-0-1-0-dev</tag>
    </release>
    <release>
      <version>0-2-0-dev</version>
      <tag>jspider-0-2-0-dev</tag>
    </release>
    <release>
      <version>0-3-0-dev</version>
      <tag>jspider-0-2-0-dev</tag>
    </release>
  </releases>
  <mailingLists>
    <mailingList>
      <name>JSpider User List</name>
      <subscribe>http://lists.sourceforge.net/lists/listinfo/j-spider-user</subscribe>
      <unsubscribe>http://lists.sourceforge.net/lists/listinfo/j-spider-user</unsubscribe>
      <archive>http://sourceforge.net/mailarchive/forum.php?forum=j-spider-user</archive>
    </mailingList>
    <mailingList>
      <name>JSpider Development List</name>
      <subscribe>http://lists.sourceforge.net/lists/listinfo/j-spider-development</subscribe>
      <unsubscribe>http://lists.sourceforge.net/lists/listinfo/j-spider-development</unsubscribe>
      <archive>http://sourceforge.net/mailarchive/forum.php?forum=j-spider-development</archive>
    </mailingList>
  </mailingLists>
  <developers>
    <developer>
      <name>Gunther Van Roey</name>
      <id>vanrogu</id>
      <email>gunther@javacoding.net</email>
      <organization>JavaCoding.net</organization>
    </developer>
  </developers>
  <dependencies>
    <dependency>
        <id>log4j</id>
        <version>1.2.8</version>
        <url>http://jakarta.apache.org/log4j</url>
    </dependency>
    <dependency>
      <id>commons-logging</id>
      <version>1.0.2</version>
      <url>http://jakarta.apache.org/commons/logging</url>
    </dependency>
    <dependency>
        <id>velocity</id>
        <version>1.3.1</version>
        <url>http://jakarta.apache.org/velocity</url>
    </dependency>
  </dependencies>
  <build>
    <nagEmailAddress>build-nag@javacoding.net</nagEmailAddress>
    <sourceDirectory>src</sourceDirectory>
    <unitTestSourceDirectory>test</unitTestSourceDirectory>
    <aspectSourceDirectories/>

    <unitTest>
      <includes>
        <include>**/*Test.java</include>
      </includes>
      <excludes>
        <exclude>**/XmlRpcTest.java</exclude>
      </excludes>
    </unitTest>

    <jars/>
  </build>
</project>
