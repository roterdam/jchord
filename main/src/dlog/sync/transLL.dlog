# name=transLL-dlog

.include "L.dom"

.bddvarorder L0xL1

###
# Relations
###

LL(l1:L0,l2:L1) input
transLL(l1:L0,l2:L1) output

###
# Constraints
###

transLL(l1,l2) :- LL(l1,l2).
transLL(l1,l2) :- transLL(l1,l3), transLL(l3,l2).

