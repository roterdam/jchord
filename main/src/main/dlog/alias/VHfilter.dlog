# name=VHfilter-dlog

.include "T.dom"
.include "H.dom"
.include "V.dom"

.bddvarorder T0_V0_T1_H0

###
# Relations
###

VT(v:V0,t:T0) input
HT(h:H0,t:T1) input
sub(t1:T1,t2:T0) input

VHfilter(v:V,h:H) output

###
# Constraints
###

VHfilter(v,h) :- VT(v,t), HT(h,s), sub(s,t).

