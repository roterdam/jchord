# name = formalInOut-dlog
#
# Relation containing each tuple (m, x) such that x is formalIn/Out of method m.
#
.include "M.dom"
.include "P.dom"
.include "X.dom"

.bddvarorder M0_P0_X0

# Relations
###

MPhead(m:M0,p:P0) input
MPtail(m:M0,p:P0) input

XP(x:X0,p:P0) input

formalIn(m:M0,x:X0) output
formalOut(m:M0,x:X0) output

###
# Constraints
###

formalIn(m,x) :- XP(x,p), MPhead(m,p).
formalOut(m,x) :- XP(x,p), MPtail(m,p).